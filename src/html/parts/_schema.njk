    "@type": "Organization",
    "name": "{{ brand }}",
    "logo": {
      "@type": "ImageObject",
      "url": "{{ baseurl }}/assets/img/logo-amp.png"
    }
  },
  "copyrightHolder": {
    "@id": "{{ baseurl }}/#publisher"
  },
  "sourceOrganization": {
    "@id": "{{ baseurl }}/#publisher"
  },
  "copyrightYear": {{ year() }},
  "isAccessibleForFree": true,
  "hasPart": {
    "@type": "WebPageElement",
    "isAccessibleForFree": true,
    "cssSelector": ".meteredContent"
  },
  "isPartOf": {
    "@type": [
      "CreativeWork",
      "Product"
    ],
