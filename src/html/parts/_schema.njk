  "video": [
    {
      "@id": "{{ baseurl }}/video/article.html"
    }
  ],
  "copyrightHolder": {
    "@id": "{{ baseurl }}/"
  },
  "sourceOrganization": {
    "@id": "{{ baseurl }}/"
  },
  "copyrightYear": {{ year() }},
  "isAccessibleForFree": true,
  "hasPart": {
    "@type": "WebPageElement",
    "isAccessibleForFree": true,
    "cssSelector": ".full-article"
  },
