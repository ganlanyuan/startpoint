{# all pages #}
{% macro media_organization () %}
<script type="application/ld+json">
{
  "@context": "http://schema.org",
  "@type": "NewsMediaOrganization",
  "name": "{{ brand }}",
  "logo": {
    "@type": "ImageObject",
    "url": "assets/img/static/logo-600x60.png",
    "height": 60,
    "width": 600
