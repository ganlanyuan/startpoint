<!-- all pages -->
{% macro organization () %}
<script type="application/ld+json">
{
  "@context": "https://schema.org",
  "@type": "Organization",
  "url": "{{ baseurl }}",
  "logo": "{{ baseurl }}/assets/img/logo.png",
  "name": "{{ brand }}"
}
</script>
{% endmacro %}






<!-- all pages -->
{% macro media_organization () %}
<script type="application/ld+json">
{
  "@context": "http://schema.org",
  "@type": "NewsMediaOrganization",
  "name": "{{ brand }}",
  "logo": {
    "@context": "http://schema.org",
    "@type": "ImageObject",
    "url": "{{ baseurl }}/images/misc/logo_rss_250x40.png",
    "height": 40,
    "width": 250
  },
  "url": "{{ baseurl }}/",
  "@id": "{{ baseurl }}/#publisher",
  "diversityPolicy": "{{ baseurl }}/diversity-and-inclusion/",
  "ethicsPolicy": "{{ baseurl }}/who-we-are/culture/standards-and-ethics/",
  "masthead": "{{ baseurl }}/interactive/2019/admin/masthead.html",
  "foundingDate": "1990-01-01",
  "sameAs": [
    {%- for item, url in social %}
    "{{ url }}"{{ ', ' if not loop.last }}
    {%- endfor %}
  ],
  "alternateName": [
    "alt name 1",
    "alt name 2",
    "alt name 3",
    "alt name 4"
  ]
  {# "sameAs": "https://en.wikipedia.org/wiki/{{ brand }}" #}
}
</script>
{% endmacro %}





<!-- all pages -->
{% macro website () %}
<script type="application/ld+json">
{
  "@context": "https://schema.org",
  "@type": "WebSite",
  "url": "{{ baseurl }}/",
  "name": "{{ brand }}",
  "potentialAction": {
    "@type": "SearchAction",
    "target": "{{ baseurl }}/search?&q={query}",
    {# "query": "required", #}
    "query-input": "required name=query"
  }
}
</script>
{% endmacro %}





