{# homepage #}
{% macro webpage (count=7) %}
<script type="application/ld+json">
{
  "@context": "http://schema.org",
  "@type": "WebPage",
  "image": [
    {%- for name, width, height in [
      ['nameplate_1200x675', 1200, 675],
      ['n_1200x900', 1200, 900],
      ['n_1200x1200', 1200, 1200]
      ] %}
    {
      "@type": "ImageObject",
      "url": "assets/img/static/{{ name }}.png",
      "width": {{ width }},
      "height": {{ height }}
    }{{- ',' if not loop.last -}}
    {%- endfor %}
  ],
  "name": "{{ brand }}",
  "mainEntity": {
    "@type": "ItemList",
    "itemListElement": [
      {%- for i in range(0, count) %}
      {
        "@type": "ListItem",
        "url": "{{ baseurl }}/news/article{{ loop.index }}.html",
        "position": {{ loop.index }}
      }{{- ',' if not loop.last -}}
      {%- endfor %}
    ],
    "numberOfItems": {{ count }}
  },
  "publisher": "{{ baseurl }}/"
}
</script>
{% endmacro %}





