<!-- article page -->
{% macro news_article () %}
<script type="application/ld+json">
{
  "@context": "https://schema.org",
  "@type": "NewsArticle",
  "description": "{{ get('P') }}",
  {# "mainEntityOfPage": {
    "@type": "WebPage",
    "@id": "{{ baseurl }}/article"
  }, #}
  "mainEntityOfPage": "{{ baseurl }}/article",
  "image": [
    {
      "@context": "http://schema.org",
      "@type": "ImageObject",
      "url": "{{ mc.src(1600, 900) }}",
      "height": 900,
      "width": 1600,
      "caption": "{{ get('P') }}"
    },
    {
      "@context": "http://schema.org",
      "@type": "ImageObject",
      "url": "{{ mc.src(2048, 1365) }}",
      "height": 1365,
      "width": 2048,
      "caption": "{{ get('P') }}"
    },
    {
      "@context": "http://schema.org",
      "@type": "ImageObject",
      "url": "{{ mc.src(1800, 1800) }}",
      "height": 1800,
      "width": 1800,
      "caption": "{{ get('P') }}"
    }
  ],
  "url": "{{ baseurl }}/2020/09/14/us/wildfires-live-updates.html",
  "inLanguage": "en",
  "author": {
    "@id": "{{ baseurl }}/#publisher"
  },
  {# "author": {
      "@type": "Person",
      "name": "{{ author }}"
    }, #}
  "dateModified": "{{ year() }}-09-14T14:24:46.000Z",
  "datePublished": "{{ year() }}-09-14T13:13:12.000Z",
  "headline": "{{ get('H') }}",
  "alternativeHeadline": "{{ get('H') }}",
