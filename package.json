{
  "name": "startpoint",
  "version": "1.0.0",
  "description": "A simple boilerplate for websites.",
  "main": "index.js",
  "type": "module",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "nodemon --watch index.js --exec node .",
    "njk": "task=njk node .",
    "htmlValidate": "task=htmlValidate node .",
    "sass": "task=sass node .",
    "cssValidate": "task=cssValidate node .",
    "js": "task=js node .",
    "image": "task=image node .",
    "amp": "task=amp node .",
    "ampValidate": "task=ampValidate node .",
    "critical": "docker exec critical node test/critical/critical.js",
    "compare": "docker exec -e var=compare puppeteer node test/puppeteer/puppeteer.js",
    "clear": "node test/puppeteer/clear.js",
    "pagespeed": "node test/pagespeed/pagespeed.js",
    "lighthouse": "node test/lighthouse/lighthouse.js"
  },
  "browserslist": [
    "> 0.01%",
    "Last 5 versions",
    "not ie <= 8"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ganlanyuan/startpoint.git"
  },
  "keywords": [],
  "author": "ganlanyuan <ganlanyuan@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ganlanyuan/startpoint/issues"
  },
  "homepage": "https://github.com/ganlanyuan/startpoint#readme",
  "devDependencies": {
    "dialog-polyfill": "^0.5.6",
    "normalize.css": "^8.0.0",
    "tiny-slider": "^2.9.2"
  }
}
