{
  "name": "startpoint",
  "version": "1.0.0",
  "description": "A simple boilerplate for websites.",
  "main": "gulpfile.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "critical": "docker exec critical node test/critical/critical.js",
    "puppeteer-save": "docker run --rm -t --env var=save --name puppeteer -p 2000:2000 --cap-add SYS_ADMIN --mount type=bind,source=\"$(pwd)\",target=/home/chrome puppeteer node test/puppeteer/puppeteer.js",
    "puppeteer-compare": "docker run --rm -t --env var=compare --name puppeteer -p 2000:2000 --cap-add SYS_ADMIN --mount type=bind,source=\"$(pwd)\",target=/home/chrome puppeteer node test/puppeteer/puppeteer.js",
    "save": "docker exec puppeteer node test/puppeteer/puppeteer.js",
    "pagespeed": "docker exec startpoint node test/pagespeed/pagespeed.js",
    "lighthouse": "docker exec lighthouse node test/lighthouse/lighthouse.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ganlanyuan/startpoint.git"
  },
  "keywords": [],
  "author": "ganlanyuan <ganlanyuan@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ganlanyuan/startpoint/issues"
  },
  "homepage": "https://github.com/ganlanyuan/startpoint#readme",
  "dependencies": {
    "optipng-bin": "^4.0.0",
    "pngquant": "^1.3.0",
    "pngquant-bin": "^4.0.0",
    "rocket-sass": "^4.0.1"
  }
}
